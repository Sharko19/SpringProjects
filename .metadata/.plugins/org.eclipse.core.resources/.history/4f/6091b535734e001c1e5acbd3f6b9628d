package com.hcl.javaannotationbased;

import org.springframework.beans.factory.annotation.Autowired;

public class Audi 
{
	@Autowired
	Engine engine;
	public Engine getEngine() {
		return engine;
	}
	public void setEngine(Engine engine) {
		this.engine = engine;
	}
	public void config()
{
	System.out.println("Automatic car, petrol");
	engine.rotate();
}
}
