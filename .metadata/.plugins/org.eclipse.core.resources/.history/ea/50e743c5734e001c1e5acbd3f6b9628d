package com.hcl.javaannotationbased;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

@Configuration
public class annoconfig 
{
	@Bean
	public Audi getCar()
	{
		return new Audi();
	}
	
	@Bean
	public Engine getEngine() 
	{
		return new Mpfi();
	} 
}
